import requests
import pandas as pd
from PIL import Image,ImageFilter


print('библиотека Python: requests')

# проверим есть ли доступ к информационному ресурсу, либо он заблокирован

requests.get("https://amedia.lol")
res = requests.get("https://amedia.lol")
print (res)

requests.get ("https://www.pochta.ru")
res_1 = requests.get("https://www.pochta.ru")
print (res_1)

requests.get ("https://igumnov.group")
res_2 = requests.get("https://igumnov.group")
print (res_2)

# получаем код страничек сайта

print (res.content)
print (res_1.content)
print (res_2.content)

# получаем инфомацию в текстовом виде, по сути не многим отличается от контента

print (res.text)
print (res_1.text)
print (res_2.text)

# получаем заголовки

print (res.headers)
print (res_1.headers)
print (res_2.headers)

# Запрос GET можно настроить с помощью передачи параметров в методе params.
# Посмотрим, как это работает на простом примере — попробуем загрузить изображение c сайта httbin.org


params = {"param_test": "param"}
data = requests.get("https://httpbin.org/image/jpeg", params=params)
print (data)
with open("test.jpg", "wb") as file:
    file.write(data.content)
    file.close()

print('библиотека Python: pandas')
df = pd.read_excel("Курсы валют к рублю 16012025.xlsx")
print (df)
print (df.head(10).sort_values("Курс"))     # десять значений из таблицы, сортируем по курсу
eng_names = {"Number code","Numeral code","Unit","Currency","Rate"}
columns = {"Цифровой код", "Буквенный код", "Единица", "Валюта", "Курс"}
# df = df.set_axis(axis=1, labels=columns)
# df.set_axis(columns, axis=1)
print(df)
# изменить название столбцов
df.set_axis(eng_names, axis="columns")
pd.set_option("display.max_columns", None)
df.rename(columns={"Цифр. код": "Number code", "Букв. код": "Literal code","Единиц": "Unit", "Валюта": "Currency", "Курс": "Rate" }, inplace=True)
print(df)


# вывести на экран названия колонок таблицы
for col in df.columns:
    print(col)

print('библиотека Python: pillow')

image = Image.open('Снеговик.jpeg')
resized_image = image.resize((800, 600))  # изменение размера на 800 x 600 пикселей
resized_image.save('resized_image.png')

image = Image.open('Снеговик.jpeg')
blurred_image = image.filter(ImageFilter.BLUR)  # применить эффекты
blurred_image.save('blurred_image.png')

# сохранить в другой формат
image = Image.open('Снеговик.jpeg')
image.save('converted_image.jpg')  # конвертация в формат JPEG
image.save('converted_image.gif')  # конвертация в формат GIF


